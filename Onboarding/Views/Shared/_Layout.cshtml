<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title </title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Content/scripts/angular1.6.1.js"></script>
    <script src="~/Content/scripts/angular-animate1.6.1.js"></script>
    <script src="~/Content/scripts/angular-sanitize1.6.1.js"></script>
    <script src="~/Content/scripts/ui-bootstrap-tpls-2.5.0.js"></script>
    @Scripts.Render("~/bundles/angularControllers")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/js")






</head>
<body ng-app="onboard-app">
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">Onboarding</a>
            </div>
            <ul class="nav navbar-nav">
                <li class="active"><a href="@Url.Action("customerIndex","Customer")">Customer Admin</a></li>
                <li><a href="@Url.Action("OrderIndex","Order")">Order Admin</a></li>
            </ul>
        </div>
    </nav>
    <div class="container">
        @RenderBody()
        <hr />
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
